{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": ".",
        "exclude": [".vscode", "data\\", "dist", "doc", "node_modules", "test"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "template": "node_modules/better-docs",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "readme": "./README.md"
    }
}